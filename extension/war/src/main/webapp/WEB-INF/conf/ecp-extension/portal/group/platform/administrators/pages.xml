<?xml version="1.0" encoding="ISO-8859-1"?>

<page-set xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_2.xsd http://www.gatein.org/xml/ns/gatein_objects_1_2"
	xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_2">
	
	<page>
		<name>addNewCompany</name>
		<title>addNewCompany</title>
		<access-permissions>manager:/platform/administrators</access-permissions>
		<edit-permission>manager:/platform/administrators</edit-permission>
		<portlet-application>
			<portlet>
				<application-ref>formgenerator</application-ref>
				<portlet-ref>FastContentCreatorBasicPortlet</portlet-ref>
				<preferences>
					<preference>
						<name>redirectPath</name>
						<value>updateCompanyInfo</value>
					</preference>
					<preference>
						<name>mode</name>
						<value>basic</value>
					</preference>
					<preference>
						<name>workspace</name>
						<value>ecp</value>
					</preference>
					<preference>
						<name>isRedirect</name>
						<value>true</value>
					</preference>
					<preference>
						<name>path</name>
						<value>/companies</value>
					</preference>
					<preference>
						<name>saveMessage</name>
						<value>Your company has been created successfully.</value>
					</preference>
					<preference>
						<name>saveButton</name>
						<value>Save</value>
					</preference>
					<preference>
						<name>type</name>
						<value>ecp:company</value>
						<read-only>false</read-only>
					</preference>
				</preferences>
			</portlet>
			<title>Add new company</title>
			<access-permissions>manager:/platform/administrators</access-permissions>
			<show-info-bar>false</show-info-bar>
			<show-application-state>false</show-application-state>
			<show-application-mode>false</show-application-mode>
		</portlet-application>
	</page>
	<page>
		<name>updateCompanyInfo</name>
		<title>updateCompanyInfo</title>
		<access-permissions>manager:/platform/administrators</access-permissions>
		<edit-permission>manager:/platform/administrators</edit-permission>
		<portlet-application>
			<portlet>
				<application-ref>ecp-portlets</application-ref>
				<portlet-ref>updateCompanyInfoPortlet</portlet-ref>
			</portlet>
			<title>Update company info</title>
			<access-permissions>manager:/platform/administrators</access-permissions>
			<show-info-bar>false</show-info-bar>
			<show-application-state>false</show-application-state>
			<show-application-mode>false</show-application-mode>
		</portlet-application>
	</page>
	<page>
		<name>addNewCategory</name>
		<title>addNewCategory</title>
		<access-permissions>manager:/platform/administrators</access-permissions>
		<edit-permission>manager:/platform/administrators</edit-permission>
		<portlet-application>
			<portlet>
				<application-ref>formgenerator</application-ref>
				<portlet-ref>FastContentCreatorBasicPortlet</portlet-ref>
				<preferences>
					<preference>
						<name>redirectPath</name>
						<value>/portal/public/ecp</value>
					</preference>
					<preference>
						<name>mode</name>
						<value>basic</value>
					</preference>
					<preference>
						<name>workspace</name>
						<value>ecp</value>
					</preference>
					<preference>
						<name>isRedirect</name>
						<value>false</value>
					</preference>
					<preference>
						<name>path</name>
						<value>/category</value>
					</preference>
					<preference>
						<name>saveMessage</name>
						<value>Your category has been created successfully.</value>
					</preference>
					<preference>
						<name>saveButton</name>
						<value>Save</value>
					</preference>
					<preference>
						<name>type</name>
						<value>ecp:category</value>
						<read-only>false</read-only>
					</preference>
				</preferences>
			</portlet>
			<title>Add new category</title>
			<access-permissions>manager:/platform/administrators</access-permissions>
			<show-info-bar>false</show-info-bar>
			<show-application-state>false</show-application-state>
			<show-application-mode>false</show-application-mode>
		</portlet-application>
	</page>
	<page>
		<name>manageSubscriptionType</name>
		<title>manageSubscriptionType</title>
		<access-permissions>manager:/platform/administrators</access-permissions>
		<edit-permission>manager:/platform/administrators</edit-permission>
		<portlet-application>
			<portlet>
				<application-ref>ecp-portlets</application-ref>
				<portlet-ref>manageSubscriptionTypePortlet</portlet-ref>
			</portlet>
			<title>Manage Subscription-Type</title>
			<access-permissions>manager:/platform/administrators</access-permissions>
			<show-info-bar>false</show-info-bar>
			<show-application-state>false</show-application-state>
			<show-application-mode>false</show-application-mode>
		</portlet-application>
	</page>
	<page>
		<name>activateUsers</name>
		<title>activateUsers</title>
		<access-permissions>manager:/platform/administrators</access-permissions>
		<edit-permission>manager:/platform/administrators</edit-permission>
		<portlet-application>
			<portlet>
				<application-ref>ecp-portlets</application-ref>
				<portlet-ref>activateUsersPortlet</portlet-ref>
			</portlet>
			<title>Activate users</title>
			<access-permissions>manager:/platform/administrators</access-permissions>
			<show-info-bar>false</show-info-bar>
			<show-application-state>false</show-application-state>
			<show-application-mode>false</show-application-mode>
		</portlet-application>
	</page>
</page-set>
